set terminal pdf
set output output_filename

set ylabel y_label
set xlabel x_label
#set xtics auto
set macros
#x_tics_string="(\"A\" 1, \"B\" 2, \"C\" 3)"
set xtics @x_tics
#set xtics rotate by 45 right

#legend
set key outside center top

set grid ytics lc rgb "#bbbbbb" lw 1 lt 0
set grid xtics lc rgb "#bbbbbb" lw 1 lt 0

if (exists("log_scale_x")){
	set logscale x log_scale_x
}

#set yrange [0:4096]

columns_num = system("awk 'NR==1{print NF}' " . data_filename) #find the number of columns in the data file

plot for [i=2:columns_num] data_filename using 1:i with linespoints title col

